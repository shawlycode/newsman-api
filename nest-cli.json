{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true
  },
  "monorepo": true,
  "projects": {
    "micro.news": {
      "type": "application",
      "root": "apps/micro.news",
      "entryFile": "main",
      "sourceRoot": "apps/micro.news/src",
      "compilerOptions": {
        "tsConfigPath": "apps/micro.news/tsconfig.app.json"
      }
    }
  }
}
